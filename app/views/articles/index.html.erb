
<h1 class="heading">Sass is bad ass</h1>
<div class="yoVendorPrefix"></div>
<div class="gameOfT"></div>

<div class="artNewLink"><%= link_to 'New article', new_article_path %></div>
<table>
  <tr>
    <th class="headTitle">Title</th>
    <th class="headText">Text</th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td class="artTitle"><%= article.title %></td>
      <td class="artText"><%= article.text %></td>
      <td class="link"><%= link_to 'Show', article_path(article) %></td>
    </tr>
  <% end %>
</table>